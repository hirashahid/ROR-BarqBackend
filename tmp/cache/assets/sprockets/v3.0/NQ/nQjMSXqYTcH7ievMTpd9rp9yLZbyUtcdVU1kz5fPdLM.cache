      var options;
      if (App.getCookie('pauseNotifications') === 'true') {
        return;
      }
      options = {
        "extendedTimeOut": 5000,
        "tapToDismiss": true,
        "closeButton": true,
        "progressBar": true,
        "timeOut": 12000
      };
      toastr.options.closeButton = true;
      toastr.options.progressBar = true;
      toastr.options.timeOut = 15000;
      if (data.url != null) {
        toastr.options.onclick = function() {
          return window.location.href = data.url;
        };
      }
      return toastr.warning(data.message, data.title);
    },
    speak: function() {
      return this.perform('speak');
    }
  });

}).call(this);
